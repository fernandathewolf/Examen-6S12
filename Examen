/*FUNCIONES PARA EJECUTAR, VER O BORRAR LA TABLA P_ISC O SISTEMAS*/
execute P_ISC;
select * from SISTEMAS;
delete from SISTEMAS;

/*CREAMOS LAS TABLAS DE BECA*/
CREATE TABLE CIEN(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);

create or replace PROCEDURE P_CIEN
as
begin
insert into CIEN (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO>=95;
end;

execute P_CIEN;
select* from CIEN;
delete from CIEN;

CREATE TABLE SETENTA(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);

create or replace PROCEDURE P_SETENTA
as
begin
insert into SETENTA (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO>=92 AND PROMEDIO_PASADO <95;
end;

execute P_SETENTA;
select* from SETENTA;
delete from SETENTA;

CREATE TABLE CINCUENTA(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);

create or replace PROCEDURE P_CINCUENTA
as
begin
insert into CINCUENTA (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO >=90 AND PROMEDIO_PASADO <92;
end;

execute P_CINCUENTA;
select* from CINCUENTA;
delete from CINCUENTA;

CREATE TABLE TREINTA(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);

create or replace PROCEDURE P_TREINTA
as
begin
insert into TREINTA (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO>=87 AND PROMEDIO_PASADO<90;
end;

execute P_TREINTA;
select* from TREINTA;
delete from TREINTA;

CREATE TABLE VEINTE(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);

create or replace PROCEDURE P_VEINTE
as
begin
insert into VEINTE (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO>=84 AND PROMEDIO_PASADO <87;
end;

execute P_VEINTE;
select* from VEINTE;
delete from VEINTE;

CREATE TABLE DIEZ(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);


create or replace PROCEDURE P_DIEZ
as
begin
insert into DIEZ (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO>=80 AND PROMEDIO_PASADO <84;
end;

execute P_DIEZ;
select* from DIEZ;
delete from DIEZ;

CREATE TABLE CERO(
ID NUMBER (30) UNIQUE,
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERA VARCHAR2 (30),
SEMESTRE NUMBER (30),
CHECK (SEMESTRE <=9)
PROMEDIO_PASADO NUMBER (30)
);

create or replace PROCEDURE P_CERO
as
begin
insert into CERO (ID, MATRICULA, NOMBRE, CARRERA, SEMESTRE, PROMEDIO_PASADO)
select ID, MATRICULA, NOMBRE ||' '|| APELLIDO_P||' '||  APELLIDO_M, CARRERA, SEMESTRE, PROMEDIO_PASADO  from ALUMNOS 
where PROMEDIO_PASADO<80;
end;

execute P_CERO;
select* from CERO;
delete from CERO;
