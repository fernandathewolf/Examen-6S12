/*CREAMOS LA TABLA DE AUTOMOTRICES*/
CREATE TABLE AUTO(
ID NUMBER(20),
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
APELLIDOS VARCHAR2 (30),
GRUPO VARCHAR2 (30),
EDIFICIO VARCHAR2 (30),
 );
 
/*CREAMOS OTRA FUNCION PARA VER SOLO LA CARRERA DE AUTOMOTRICES*/
create or replace procedure A_AUTO
as
begin
insert into AUTO (ID, MATRICULA, NOMBRE, APELLIDOS, GRUPO, EDIFICIO)
select ID, MATRICULA, NOMBRE, APELLIDO_P | | ' ' | |  APELLIDO_M, GRUPO, EDIFICIO from ALUMNOS
where Carrera = 'AUTOMOTRICES';
end;

/*FUNCIONES PARA EJECUTAR, VER O BORRAR LA TABLA A_AUTO O AUTO*
execute A_AUTO;
select* from AUTO;
delete from AUTO;

/*CREAMOS LA TABLA DE INDUSTRIAL*/
CREATE TABLE IND(
ID NUMBER(20),
MATRICULA NUMBER (30),
NOMBRE VARCHAR2 (30),
APELLIDOS VARCHAR2 (30),
GRUPO VARCHAR2 (30),
EDIFICIO VARCHAR2 (30),
 );
 
/*CREAMOS OTRA FUNCION PARA VER SOLO LA CARRERA DE INDUSTRIAL*/
create or replace procedure A_IND
as
begin
insert into IND (ID, MATRICULA, NOMBRE, APELLIDOS, GRUPO, EDIFICIO)
select ID, MATRICULA, NOMBRE, APELLIDO_P | | ' ' | |  APELLIDO_M, GRUPO, EDIFICIO from ALUMNOS
where Carrera = 'INDUSTRIAL';
end;

/*FUNCIONES PARA EJECUTAR, VER O BORRAR LA TABLA A_IND O IND*
execute A_IND;
select* from IND;
delete from IND;

