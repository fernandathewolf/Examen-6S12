/*CREAMOS UNA TABLA DE PROFESORES CON LOS DATOS SOLICITADOS*/
CREATE TABLE PROFESORES(
ID NUMBER(20),
PRIMARY KEY (id),
NUM_EMPLEADO NUMBER (30) UNIQUE,
NOMBRE VARCHAR2 (30) NOT NULL,
APELLIDO_P VARCHAR2 (30) NOT NULL,
APELLIDO_M VARCHAR2 (30) NOT NULL,
CARRERAS_IMPARTIDAS VARCHAR2 (30) NOT NULL,
NOMBRE_CLASES VARCHAR2 (30) NOT NULL,
GRUPOS VARCHAR2 (30) NOT NULL
);

/*PODEMOS SELECCIONAR VER LA TABLA O BORRARLA*/
select * from PROFESORES;
delete from PROFESORES;

/*AQUI CREAMOS UNA FUNCION CON LOS DATOS QUE SOLICITAMOS*/
create or replace procedure IP
(M_ID IN NUMBER DEFAULT 0, M_NUM_EMPLEADO IN NUMBER DEFAULT 0, M_NOMBRE IN VARCHAR2 DEFAULT 'PROFESOR', M_APELLIDO_P IN VARCHAR2 DEFAULT 'APELLIDO_P', 
 M_APELLIDO_M IN VARCHAR2 DEFAULT 'APELLIDO_M', M_CARRERAS_IMPARTIDAS IN VARCHAR2 DEFAULT 'CARRERA', M_NOMBRE_CLASES IN VARCHAR2 DEFAULT 'CLASE', M_GRUPOS IN VARCHAR2 DEFAULT 'GRUPOS')
 as 
 begin 
 insert into PROFESORES values (M_ID, M_NUM_EMPLEADO, M_NOMBRE, M_APELLIDO_P, M_APELLIDO_M, M_CARRERAS_IMPARTIDAS, M_NOMBRE_CLASES, M_GRUPOS);
 end;

