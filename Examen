CREATE TABLE ALUMNOS(
ID NUMBER(20),
PRIMARY KEY (id),
MATRICULA NUMBER (30) UNIQUE,
NOMBRE VARCHAR2 (30) NOT NULL,
APELLIDO_P VARCHAR2 (30),
APELLIDO_M VARCHAR2 (30),
CARRERA VARCHAR2 (30),
GRUPO VARCHAR2 (30),
SEMESTRE NUMBER (30),
EDIFICIO VARCHAR2 (30),
PROMEDIO_PASADO NUMBER (30)
CHECK (PROMEDIO_PASADO<=100)
); /* CREAMOS UNA TABLA DE ALUMNOS CON LOS DATOS SOLICITADOS*/

select * from ALUMNOS;
delete from ALUMNOS;
/*PODEMOS SELECCIONAR VER LA TABLA O BORRARLA*/

 create or replace procedure IA
(M_ID IN NUMBER DEFAULT 0, M_MATRICULA IN NUMBER DEFAULT 0, M_NOMBRE IN VARCHAR2 DEFAULT 'NOMBRE', M_APELLIDO_P IN VARCHAR2 DEFAULT 'APELLIDO_P', M_APELLIDO_M IN VARCHAR2 DEFAULT 'APELLIDO_M',
 M_CARRERA IN VARCHAR2 DEFAULT 'CARRERA ', M_GRUPO IN VARCHAR2 DEFAULT '0000', M_SEMESTRE IN NUMBER DEFAULT 1, A_EDIFICIO IN VARCHAR2 DEFAULT '--', M_PROMEDIO_PASADO IN NUMBER DEFAULT 0)
 as 
 begin 
 insert into ALUMNOS values (M_ID, M_MATRICULA, M_NOMBRE, M_APELLIDO_P, M_APELLIDO_M, M_CARRERA, M_GRUPO, M_SEMESTRE, M_EDIFICIO, M_PROMEDIO_PASADO);
 end;
