/*CREAMOS LA TABLA DE AUTOMOTRICES*/
CREATE TABLE AUTOMOTRICES(
ID NUMBER NOT NULL UNIQUE,
NUM_EMPLEADO NUMBER (30),
NOMBRE VARCHAR2 (30),
CARRERAS_IMPARTIDAS VARCHAR2 (30),
NOMBRE_CLASES VARCHAR2 (30),
GRUPOS VARCHAR2 (30) 
);
 
/*CREAMOS OTRA FUNCION PARA VER SOLO LA CARRERA DE AUTOMOTRICES*/
create or replace procedure P_AUTO
as
begin
insert into AUTOMOTRICES (ID, NUM_EMPLEADO, NOMBRE, CARRERAS_IMPARTIDAS, NOMBRE_CLASES, GRUPOS)
select ID, NUM_EMPLEADO, NOMBRE | |' '| | APELLIDO_P | | ' ' | |  APELLIDO_M, CARRERAS_IMPARTIDAS, NOMBRE_CLASES, GRUPOS  from PROFESORES
where Carrera = 'AUTOMOTRICES';
end;

/*FUNCIONES PARA EJECUTAR, VER O BORRAR LA TABLA P_AUTO O AUTOMOTRICES*
execute P_AUTO;
select* from AUTOMOTRICES;
delete from AUTOMOTRICES;

